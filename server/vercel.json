{
  "version": 2,
  "rewrites": [
    { "source": "/(.*)", "destination": "index.js" }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/", "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }}
  ]
}
